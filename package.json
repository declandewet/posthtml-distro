{
  "name": "reshape-rss",
  "description": "Reshape plugin for generating RSS feeds",
  "version": "0.0.0",
  "author": "Declan de Wet <declandewet@me.com>",
  "ava": {
    "verbose": true
  },
  "bugs": {
    "url": "https://github.com/declandewet/reshape-rss/issues"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "codecov": "^1.0.1",
    "nyc": "^14.1.0",
    "reshape": "^0.3.1",
    "standard": "^8.0.0-beta.5"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/declandewet/reshape-rss#readme",
  "keywords": [
    "feed",
    "html",
    "reshape",
    "reshape-plugin",
    "rss"
  ],
  "license": "MIT",
  "main": "index.js",
  "nyc": {
    "cache": true,
    "all": true,
    "include": [
      "index.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/declandewet/reshape-rss.git"
  },
  "scripts": {
    "codecov": "codecov",
    "coverage": "nyc report",
    "lint": "standard",
    "precodecov": "npm run coverage",
    "pretest": "npm run lint",
    "test": "nyc ava"
  },
  "standard": {
    "verbose": true
  }
}
